# Settings for application
## APP_NAME
APP_NAME=NCC-DEV
## APP_ENV
APP_ENV=local
## APP_DEBUG
APP_DEBUG=true
## APP_KEY : This value is generated by "php artisan key:generate" command
APP_KEY=
## APP_URL : Please modify according to the installation environment. ex) http://192.168.10.10:8080
APP_URL=

# Settings for laravel/sail
## APP_SERVICE : This value must match the service name in "docker-compose.yml"
APP_SERVICE=laravel.ncc
## WWWGROUP : Please modify according to the installation environment
WWWGROUP=1000
## WWWUSER : Please modify according to the installation environment
WWWUSER=1000
## APP_PORT : If the port number conflicts, etc., set the following variables as appropriate.
APP_PORT=8080
## FORWARD_DB_PORT : If the port number conflicts, etc., set the following variables as appropriate.
FORWARD_DB_PORT=5432
## FORWARD_REDIS_PORT : If the port number conflicts, etc., set the following variables as appropriate.
FORWARD_REDIS_PORT=6379
## FORWARD_MAILHOG_PORT : If the port number conflicts, etc., set the following variables as appropriate.
FORWARD_MAILHOG_PORT=1025
## FORWARD_MAILHOG_DASHBOARD_PORT : If the port number conflicts, etc., set the following variables as appropriate.
FORWARD_MAILHOG_DASHBOARD_PORT=8025

# DATABASE
## DB_CONNECTION : do not modify.
DB_CONNECTION=pgsql
## DB_HOST : This value must match the postgresql service name in "docker-compose.yml"
DB_HOST=pgsql
## DB_PORT : do not modify.
DB_PORT=5432
## DB_DATABASE : If you change this value, you will need to rebuild the container.
DB_DATABASE=laravel
## DB_USERNAME : If you change this value, you will need to rebuild the container.
DB_USERNAME=sail
## DB_PASSWORD : If you change this value, you will need to rebuild the container.
DB_PASSWORD=password

# Redis
## REDIS_HOST : This value must match the redis service name in "docker-compose.yml"
REDIS_HOST=redis
## REDIS_PASSWORD
REDIS_PASSWORD=null
## REDIS_PORT : do not modify.
REDIS_PORT=6379

# Mail
MAIL_MAILER=smtp
MAIL_HOST=mailhog
MAIL_PORT=1025
MAIL_USERNAME=null
MAIL_PASSWORD=null
MAIL_ENCRYPTION=null
MAIL_FROM_ADDRESS=null
MAIL_FROM_NAME="${APP_NAME}"

# AWS
AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_DEFAULT_REGION=us-east-1
AWS_BUCKET=
AWS_USE_PATH_STYLE_ENDPOINT=false

# PUSHER
PUSHER_APP_ID=
PUSHER_APP_KEY=
PUSHER_APP_SECRET=
PUSHER_APP_CLUSTER=mt1

MIX_PUSHER_APP_KEY="${PUSHER_APP_KEY}"
MIX_PUSHER_APP_CLUSTER="${PUSHER_APP_CLUSTER}"

# Cache
CACHE_DRIVER=redis

# Session
SESSION_DRIVER=database
SESSION_LIFETIME=120

# Queue
QUEUE_CONNECTION=sync

# Others
BROADCAST_DRIVER=log
FILESYSTEM_DRIVER=local
MEMCACHED_HOST=127.0.0.1
